
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:id="@+id/activity_main"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:orientation="vertical"
tools:context="com.example.eider.carpoolinguniversitario.Fragmentos.ThreeFragment">


    <TextView
        android:id="@+id/horarioactual"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ACTUAL"
        android:textSize="20dp"
        android:textStyle="bold"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/horario"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="23dp"
        app:layout_constraintLeft_toRightOf="@+id/horario" />

    <TextView
        android:id="@+id/fechaactual"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ACTUAL"
        android:textSize="20dp"
        android:textStyle="bold"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="11dp"
        android:layout_marginTop="32dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/Fecha"
        app:layout_constraintLeft_toRightOf="@+id/Fecha"
        android:layout_marginLeft="11dp" />

    <TextView
        android:textSize="20dp"
        android:textStyle="bold"
        android:text="ACTUAL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/edit_Destino"
        android:layout_marginEnd="53dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="96dp"
        android:layout_marginRight="53dp" />
<android.support.design.widget.TextInputLayout
    android:id="@+id/horario"
    android:layout_width="127dp"
    android:layout_height="79dp"
    android:layout_marginLeft="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginTop="86dp"
    android:padding="12sp"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/edit_Destino"
    tools:layout_constraintLeft_creator="1"
    tools:layout_constraintRight_creator="1"
    tools:layout_constraintTop_creator="1">

    <EditText
        android:id="@+id/input_horario"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Horario"
        android:inputType="phone"
        android:textColorHint="#05ab9a" />

</android.support.design.widget.TextInputLayout>

<android.support.design.widget.TextInputLayout
    android:id="@+id/plazas"
    android:layout_width="127dp"
    android:layout_height="79dp"
    android:layout_marginLeft="8dp"
    android:layout_marginStart="8dp"
    android:padding="12sp"
    app:layout_constraintHorizontal_bias="0.935"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/edit_Destino"
    tools:layout_constraintLeft_creator="1"
    tools:layout_constraintRight_creator="1"
    tools:layout_constraintTop_creator="1"
    android:layout_marginTop="86dp">

    <EditText
        android:id="@+id/input_plazas"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Plazas"
        android:inputType="phone"
        android:textColorHint="#05ab9a" />

</android.support.design.widget.TextInputLayout>

<android.support.design.widget.TextInputLayout
    android:id="@+id/Fecha"
    android:layout_width="244dp"
    android:layout_height="93dp"
    android:padding="12sp"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/edit_Destino"
    tools:layout_constraintLeft_creator="1"
    tools:layout_constraintRight_creator="1"
    tools:layout_constraintTop_creator="1">

    <EditText
        android:id="@+id/input_fecha"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Fecha"
        android:inputType="phone"
        android:textColorHint="#05ab9a" />

</android.support.design.widget.TextInputLayout>

<TextView
    android:id="@+id/textView"
    android:layout_width="0dp"
    android:layout_height="34dp"
    android:layout_marginBottom="21dp"
    android:layout_marginEnd="21dp"
    android:layout_marginLeft="21dp"
    android:layout_marginRight="21dp"
    android:layout_marginStart="21dp"
    android:layout_marginTop="7dp"
    android:hint="Publicar Viaje"
    android:textAlignment="center"
    android:textColor="@color/colorPrimary"
    android:textSize="25dp"
    app:layout_constraintBottom_toTopOf="@+id/edit_Destino"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.0"
    tools:layout_constraintBottom_creator="1"
    tools:layout_constraintLeft_creator="1"
    tools:layout_constraintRight_creator="1"
    tools:layout_constraintTop_creator="1" />


<android.support.design.widget.TextInputLayout
    android:id="@+id/edit_Origen"
    android:padding="12sp"
    android:layout_width="246dp"
    android:layout_height="82dp"
    tools:layout_conversion_absoluteX="16dp"
    tools:layout_conversion_absoluteY="16dp"
    tools:layout_conversion_absoluteWidth="352dp"
    tools:layout_conversion_absoluteHeight="54dp"
    tools:layout_constraintTop_creator="1"
    tools:layout_constraintRight_creator="1"
    app:layout_constraintRight_toRightOf="parent"
    android:layout_marginTop="59dp"
    tools:layout_constraintLeft_creator="1"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintHorizontal_bias="0.0">

    <EditText
        android:id="@+id/input_origen"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Origen"
        android:textColorHint="#05ab9a" />
</android.support.design.widget.TextInputLayout>

<!-- un00000000000000 -->
    <!-- doooooooooooooooooooooooooooooooooooooooooos -->


<Button
    android:id="@+id/publicar_viaje"
    android:layout_width="220dp"
    android:layout_height="50dp"
    android:text="Publicar Viaje "
    android:background="@color/colorPrimary"
    android:textColor="@color/Clouds"
    android:textSize="22dp"
    android:theme="@style/MyButton"
    style="@style/Widget.AppCompat.Button.Borderless"
    tools:layout_conversion_absoluteX="148dp"
    tools:layout_conversion_absoluteY="441dp"
    tools:layout_conversion_absoluteWidth="220dp"
    tools:layout_conversion_absoluteHeight="50dp"
    tools:layout_constraintRight_creator="1"
    tools:layout_constraintBottom_creator="1"
    app:layout_constraintBottom_toBottomOf="parent"
    android:layout_marginEnd="16dp"
    app:layout_constraintRight_toRightOf="parent"
    android:layout_marginBottom="20dp"
    android:layout_marginRight="16dp"
    app:layout_constraintVertical_bias="0.973" />

    <android.support.design.widget.TextInputLayout
        android:id="@+id/edit_Destino"
        android:layout_width="247dp"
        android:layout_height="72dp"
        android:layout_marginTop="-8dp"
        android:padding="12sp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/edit_Origen"
        app:layout_constraintRight_toRightOf="@+id/edit_Origen"
        app:layout_constraintTop_toBottomOf="@+id/edit_Origen"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_conversion_absoluteHeight="54dp"
        tools:layout_conversion_absoluteWidth="352dp"
        tools:layout_conversion_absoluteX="16dp"
        tools:layout_conversion_absoluteY="70dp">

        <EditText
            android:id="@+id/input_destino"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Destino"
            android:textColorHint="#05ab9a" />
    </android.support.design.widget.TextInputLayout>


</android.support.constraint.ConstraintLayout>
